<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/companyText" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="30dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="70dp" android:layout_marginStart="70dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="36sp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/positionText" android:layout_marginTop="56dp"
            app:layout_constraintTop_toBottomOf="@+id/companyText" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="68dp" android:layout_marginStart="68dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/nameText"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            app:layout_constraintStart_toEndOf="@+id/positionText"
            android:layout_marginLeft="120dp" android:layout_marginStart="120dp"
            app:layout_constraintBottom_toBottomOf="@+id/positionText" android:textSize="30sp"
    />
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/postalText" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="196dp"
            app:layout_constraintTop_toTopOf="parent" android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textColor="#000000" app:layout_constraintHorizontal_bias="0.116"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/addressText" app:layout_constraintStart_toStartOf="@+id/postalText"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/postalText"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:textColor="#000000"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/telText" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/addressText"
            app:layout_constraintStart_toStartOf="@+id/addressText"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:textColor="#000000"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:id="@+id/faxText"
            app:layout_constraintStart_toEndOf="@+id/telText"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            app:layout_constraintTop_toTopOf="@+id/telText"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:textColor="#000000"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/emailText"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/telText" app:layout_constraintStart_toStartOf="@+id/telText"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:textColor="#000000"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/urlText"
            app:layout_constraintStart_toEndOf="@+id/emailText"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
            app:layout_constraintTop_toTopOf="@+id/emailText"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" android:textColor="#000000"/>
</androidx.constraintlayout.widget.ConstraintLayout>